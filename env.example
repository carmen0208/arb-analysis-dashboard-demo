# DexAI Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# =============================================================================
# API Keys and Credentials
# =============================================================================
API_KEY=YOUR CUSTOMED KEY TO CONTROL THE ACCESS

# OKX DEX API Credentials
OKX_ACCESS_DEX_CONFIGS=OKX_ACCESS_DEX_API_KEY:OKX_ACCESS_DEX_SECRET_KEY:OKX_ACCESS_DEX_PASSPHRASE:OKX_ACCESS_DEX_PROJECT_ID;

# Binance API Credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# Bybit API Credentials
BYBIT_API_KEY=your_bybit_api_key_here
BYBIT_SECRET_KEY=your_bybit_secret_key_here

# Bitget API Credentials
BITGET_API_KEY=your_bitget_api_key_here
BITGET_SECRET_KEY=your_bitget_secret_key_here
BITGET_API_PASS=your_bitget_api_pass_here

# Moralis API Credentials
MORALIS_API_KEY=your_moralis_api_key_here

# CoinGecko API Credentials (Optional)
COINGECKO_API_KEY=your_coingecko_api_key_here

# Redis URL
REDIS_URL=redis://localhost:6379
