{
  "name": "@dex-ai/api-clients",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.mts",
    "test:integration:watch": "vitest run --config vitest.integration.config.mts"
  },
  "dependencies": {
    "@dex-ai/core": "workspace:*",
    "@moralisweb3/common-evm-utils": "^2.27.2",
    "@octokit/graphql": "^7.1.1",
    "@octokit/rest": "^20.0.2",
    "@pancakeswap/sdk": "^5.8.14",
    "@pancakeswap/smart-router": "^7.2.5",
    "@pancakeswap/tokens": "^0.7.5",
    "@socialdata/sdk": "0.1.0-alpha.4",
    "@the-convocation/twitter-scraper": "^0.16.3",
    "@uniswap/v3-sdk": "^3.25.2",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.8.4",
    "binance": "^3.0.0",
    "bitget-api": "^2.3.5",
    "bybit-api": "^4.1.10",
    "crypto-js": "^4.2.0",
    "graphql-request": "5.0.0",
    "jsbi": "^4.3.2",
    "moralis": "^2.27.2",
    "viem": "2.30.5",
    "ws": "^8.18.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.6.3",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.0.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "rimraf": "^5.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2",
    "vitest": "^3.0.9"
  }
}
